
type Knex = Object & Function;

type Knex$Transaction =
{
  transaction: Object,
  savepoint:   Function,
};

type Knex$Transaction$Optional
= Knex$Transaction
| void
| null
| Symbol; /* NOTX */

type Knex$Query
= Object /* non-sealed */
&
Promise<mixed>
&
{
  select: Function,
  insert: Function,
  update: Function,
  delete: Function,

  join: Function,

  toQuery: Function,
};
