{
  "name":    "knexed",
  "version": "0.3.0",

  "description": "knex utilities",

  "author":     "Strider <teamfortresslife@gmail.com>",
  "license":    "MIT",
  "repository": "StreetStrider/knexed",

  "peerDependencies":
  {
    "knex": ">= 0.10"
  },

  "scripts":
  {
    "st":    "eslint *.js tx/ join/ test/",
    "flow":  "flow",

    "test":                  "mocha    --check-leaks --recursive test/**.test.js test/**/*.test.js",
    "cover": "istanbul cover _mocha -- --check-leaks --recursive test/**.test.js test/**/*.test.js",

    "check": "npm run st && npm run flow",
    "all":   "npm run check && npm run cover",

    "coveralls": "istanbul-coveralls"
  },

  "devDependencies":
  {
    "knex":
      "0.12",

    "mocha":
      "3",

    "chai":
      "^3.5",

    "sqlite3":
      "3",

    "lodash":
      "4",

    "istanbul":
      "0.4",

    "istanbul-coveralls":
      "1",

    "flow-bin":
      "0.32",

    "eslint":
      "^3.5",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.0.8"
  }
}
